<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mysite">
    <insert id="insert" parameterType="com.javaex.vo.User">
        <![CDATA[
        INSERT INTO web 
        VALUES (NULL, #{userId}, #{userName}, #{userPw}, #{userGender})
        ]]>
    </insert>
    
    <select id="selectOne" parameterType="com.javaex.vo.User" resultType="com.javaex.vo.User">
		<![CDATA[ select web_id, web_name, web_pw, web_gender from web where web_id = #{userId} and web_pw = #{userPw} ]]>
	</select>
	
	
</mapper>
